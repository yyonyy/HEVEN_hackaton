<launch>
  <!-- Arguments -->
  <arg name="map_file" default="$(find omo_r1mini_navigation)/maps/map_hack.yaml"/>
  <arg name="move_forward_only" default="true"/>

  <!-- omo r1mini -->
  <include file="$(find omo_r1mini_description)/launch/description.launch"/>

  <!-- Map server -->
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>

  <!-- AMCL -->
  <include file="$(find omo_r1mini_navigation)/launch/amcl.launch"/>

  <!-- move_base -->
  <!-- <include file="$(find omo_r1mini_navigation)/launch/move_base.launch">
    <arg name="move_forward_only" value="$(arg move_forward_only)"/>
  </include> -->
  
  <!-- launching rviz node -->
  <node pkg="rviz" type="rviz" name="rviz" required="true" args="-d $(find omo_r1mini_navigation)/rviz/omo_r1mini_navigation.rviz"/>

</launch>
