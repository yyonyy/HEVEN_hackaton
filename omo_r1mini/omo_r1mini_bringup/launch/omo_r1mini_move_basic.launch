<launch>


  <node pkg="move_basic" type="move_basic" respawn="false" name="move_basic" output="screen">

  <param name="robot_width" value="0.09"/>
  <param name="robot_front_length" value="0.03"/>
  <param name="robot_back_length" value="0.15"/>

  <param name="lateral_kp" value="5.0"/>
  <param name="lateral_ki" value="0.0"/>
  <param name="lateral_kd" value="60.0"/>

  <param name="min_turning_velocity"             value="0.01"/>
  <param name="max_turning_velocity"             value="0.2"/>
  <param name="max_lateral_velocity"             value="0.1"/>
  <param name="max_linear_velocity"             value="0.2"/>
  <param name="min_linear_velocity"             value="0.01"/>
  <param name="forward_obstacle_threshold"       value="0.1"/>
  <param name="preferred_driving_frame"             value="map"/>

  </node>

</launch>